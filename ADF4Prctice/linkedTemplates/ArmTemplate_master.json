{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ADF4Prctice"
		},
		"LS_Blob_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_Blob'"
		},
		"LS_SQLDB_PvtEndPoint_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_SQLDB_PvtEndPoint'"
		},
		"BlobPvtEndPoint_properties_privateLinkResourceId": {
			"type": "string",
			"defaultValue": "/subscriptions/a3863a3c-c5e8-4e9e-ac98-72988254b90f/resourceGroups/ADF-Practice/providers/Microsoft.Storage/storageAccounts/blob4adf"
		},
		"BlobPvtEndPoint_properties_groupId": {
			"type": "string",
			"defaultValue": "blob"
		},
		"BlobPvtEndPoint_properties_fqdns": {
			"type": "array",
			"defaultValue": [
				"blob4adf.blob.core.windows.net"
			]
		},
		"SQLDB_PvtEndPoint_properties_privateLinkResourceId": {
			"type": "string",
			"defaultValue": "/subscriptions/a3863a3c-c5e8-4e9e-ac98-72988254b90f/resourceGroups/ADF-Practice/providers/Microsoft.Sql/servers/sqlservershankar"
		},
		"SQLDB_PvtEndPoint_properties_groupId": {
			"type": "string",
			"defaultValue": "sqlServer"
		},
		"SQLDB_PvtEndPoint_properties_fqdns": {
			"type": "array",
			"defaultValue": [
				"sqlservershankar.database.windows.net"
			]
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ADF4Prctice_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_Blob_connectionString": {
						"value": "[parameters('LS_Blob_connectionString')]"
					},
					"LS_SQLDB_PvtEndPoint_connectionString": {
						"value": "[parameters('LS_SQLDB_PvtEndPoint_connectionString')]"
					},
					"BlobPvtEndPoint_properties_privateLinkResourceId": {
						"value": "[parameters('BlobPvtEndPoint_properties_privateLinkResourceId')]"
					},
					"BlobPvtEndPoint_properties_groupId": {
						"value": "[parameters('BlobPvtEndPoint_properties_groupId')]"
					},
					"BlobPvtEndPoint_properties_fqdns": {
						"value": "[parameters('BlobPvtEndPoint_properties_fqdns')]"
					},
					"SQLDB_PvtEndPoint_properties_privateLinkResourceId": {
						"value": "[parameters('SQLDB_PvtEndPoint_properties_privateLinkResourceId')]"
					},
					"SQLDB_PvtEndPoint_properties_groupId": {
						"value": "[parameters('SQLDB_PvtEndPoint_properties_groupId')]"
					},
					"SQLDB_PvtEndPoint_properties_fqdns": {
						"value": "[parameters('SQLDB_PvtEndPoint_properties_fqdns')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}